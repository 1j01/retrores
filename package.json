{
	"name": "retrores",
	"description": "Resources from Windows 98",
	"version": "0.0.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/1j01/retrores.git"
	},
	"bugs": {
		"url": "https://github.com/1j01/retrores/issues"
	},
	"license": "UNLICENSED",
	"private": true,
	"keywords": [
		"windows",
		"98",
		"98.js",
		"windows-98",
		"operating-system",
		"os",
		"resources",
		"icons",
		"images",
		"cursors",
		"ico",
		"cur",
		"ani",
		"avi",
		"dll"
	],
	"scripts": {
		"dev": "node server.js",
		"build": "next build",
		"start": "NODE_ENV=production node server.js",
		"export": "next export"
	},
	"watch": {
		"build": {
			"patterns": [
				".",
				"templates"
			],
			"extensions": "js,jsx,html",
			"ignore": "docs/",
			"legacyWatch": true,
			"delay": 500
		}
	},
	"dependencies": {
		"@zeit/next-preact": "0.1.0",
		"express": "4.16.4",
		"next": "7.0.2",
		"preact": "8.4.2",
		"preact-compat": "3.18.4",
		"react": "16.6.3",
		"react-dom": "16.6.3",
		"slug": "0.9.2"
	}
}
